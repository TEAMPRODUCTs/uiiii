.index-date-day{
    color: #3D8D35;
    font-size: 14px;
    position: relative;
    top:10px;
    left:-25px;
}
.index-park-content{
    font-size: 14px;
    color: #a9a9a9;
    font-weight: normal;
}
.index-city-content:after,.index-date-content:after,.index-park-content:after{
    content: '';
    position: absolute;
    width: 10px;
    height: 10px;
    right: 20px;
    top: 69px;
    margin-top: -5px;
    border-top: 2px solid #a9a9a9;
    border-right: 2px solid #a9a9a9;
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
}
.index-date-content:after{
    top: 119px;
}
.index-park-content:after{
    top: 169px;
}
